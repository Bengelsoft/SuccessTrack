{
  "name": "success-tracker",
  "version": "0.0.42",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "startWithGitPod": "ng serve --disable-host-check",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky install",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output src/assets/i18n/nl.json --clean --format json",
    "commit": "git-cz",
    "release": "standard-version --no-verify",
    "generateTempChangelog": "standard-changelog -o tempCHANGELOG.md"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.8",
    "@angular/common": "^16.1.8",
    "@angular/compiler": "^16.1.8",
    "@angular/core": "^16.1.8",
    "@angular/forms": "^16.1.8",
    "@angular/platform-browser": "^16.1.8",
    "@angular/platform-browser-dynamic": "^16.1.8",
    "@angular/router": "^16.1.8",
    "@cucumber/cucumber": "^9.2.0",
    "@fullcalendar/angular": "^6.1.8",
    "@fullcalendar/core": "^6.1.8",
    "@fullcalendar/daygrid": "^6.1.8",
    "@fullcalendar/interaction": "^6.1.8",
    "@fullcalendar/timegrid": "^6.1.8",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@types/uuid": "^9.0.2",
    "chart.js": "^4.3.0",
    "class-transformer": "^0.5.1",
    "date-fns": "^2.30.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^16.1.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "resolutions": {
    "@angular/common": "^16.1.8",
    "@angular/core": "^16.1.8",
    "@angular/forms": "^16.1.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.8",
    "@angular/cli": "~16.1.8",
    "@angular/compiler-cli": "^16.1.8",
    "@commitlint/cli": "^17.7.0",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/cz-commitlint": "^17.7.0",
    "@playwright/test": "^1.36.2",
    "@types/jasmine": "~4.3.0",
    "@vendure/ngx-translate-extract": "^8.2.1",
    "commitizen": "^4.3.0",
    "husky": "^8.0.3",
    "inquirer": "^8.2.6",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "playwright": "^1.35.1",
    "standard-changelog": "^3.0.0",
    "standard-version": "^9.5.0",
    "typescript": "~5.0.2"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
